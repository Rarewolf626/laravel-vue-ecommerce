{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^4.0.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/framework": "^v11.0.7",
        "laravel/sanctum": "^v4.0.0",
        "laravel/tinker": "^v2.9.0",
        "spatie/laravel-sluggable": "^3.6.0",
        "stripe/stripe-php": "^v13.14.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.12.2",
        "fakerphp/faker": "^v1.23.1",
        "laravel/breeze": "^v2.0.0",
        "laravel/sail": "^v1.29.1",
        "laravel/telescope": "^v5.0.0",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/collision": "^v8.1.1",
        "phpunit/phpunit": "^11.0.7",
        "spatie/laravel-ignition": "^2.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
